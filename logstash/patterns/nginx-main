NGINXACCESSCUSTOM %{IPORHOST:client_ip} - %{NOTSPACE:remote_user} -> %{NOTSPACE:upstream_addr} \[%{HTTPDATE:timestamp}\] %{QS:http_host} \"(?:%{WORD:http_method} %{URIPATH:request_uri}(?:%{URIPARAM:query_string})?(?: HTTP/%{NUMBER:http_version})?|%{DATA:rawrequest})\" %{NUMBER:status_code} (?:%{NUMBER:response_bytes}|-) (%{DATA:request_body}) %{QS:referrer} %{QS:user_agent} %{NOTSPACE:http_x_forwarded_for} %{NUMBER:request_time:float} %{NOTSPACE:upstream_response_time}
